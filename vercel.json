{
	"version": 2,
	"builds": [
		{
			"src": "src/server.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api-docs/swagger-ui.css",
			"dest": "src/server.ts"
		},
		{
			"src": "/api-docs/swagger-ui-bundle.js",
			"dest": "src/server.ts"
		},
		{
			"src": "/api-docs/swagger-ui-standalone-preset.js",
			"dest": "src/server.ts"
		},
		{
			"src": "/api-docs",
			"dest": "src/server.ts"
		},
		{
			"src": "/api-docs/(.*)",
			"dest": "src/server.ts"
		},
		{
			"src": "/api/(.*)",
			"dest": "src/server.ts"
		},
		{
			"src": "/(.*)",
			"dest": "src/server.ts"
		}
	]
}
