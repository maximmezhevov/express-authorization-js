{
	"version": 2,
	"builds": [
		{
			"src": "src/server.ts",
			"use": "@vercel/node"
		},
		{
			"src": "public/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/swagger-ui/(.*)",
			"dest": "/public/swagger-ui/$1",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
				"Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
			}
		},
		{
			"src": "/api/(.*)",
			"dest": "src/server.ts",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
				"Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
			}
		},
		{
			"src": "/(.*)",
			"dest": "src/server.ts",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
				"Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
			}
		}
	]
}
