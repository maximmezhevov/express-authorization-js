{
	"version": 2,
	"builds": [
		{
			"src": "src/server.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": [
					"node_modules/swagger-ui-dist/**",
					"src/**/*"
				]
			}
		}
	],
	"routes": [
		{
			"src": "/api-docs/swagger-ui/(.*)",
			"dest": "src/server.ts"
		},
		{
			"src": "/api-docs/(.*)",
			"dest": "src/server.ts"
		},
		{
			"src": "/api/(.*)",
			"dest": "src/server.ts"
		},
		{
			"src": "/(.*)",
			"dest": "src/server.ts"
		}
	]
}
